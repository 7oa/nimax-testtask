.catalog {
	display: flex;
	flex-wrap: wrap;
	justify-content: flex-start;
	align-items: flex-start;
	@media (width >= 1300px) {
		margin: 0 -30px;
	}
	@media (width < 1300px) and (width >= 480px) {
		margin: 0 -20px;
	}
	@media (width >= 480px) {
		margin-top: -90px;
	}
	@media (width < 480px) {
		margin-top: -30px;
	}
}